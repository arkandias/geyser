{
  "name": "@geyser/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "codegen:schema": "graphql-codegen --config codegen.schema.ts",
    "codegen:documents": "graphql-codegen --config codegen.documents.ts",
    "codegen": "pnpm codegen:schema && pnpm codegen:documents",
    "locales:check": "tsx --tsconfig tsconfig.node.json src/locales/check.ts"
  },
  "dependencies": {
    "@geyser/shared": "workspace:*",
    "@urql/vue": "^1.4.3",
    "dompurify": "^3.2.5",
    "graphql": "^16.10.0",
    "keycloak-js": "^26.2.0",
    "papaparse": "^5.5.2",
    "quasar": "^2.18.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@quasar/vite-plugin": "^1.9.0",
    "@types/papaparse": "^5.3.15",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@urql/devtools": "^2.0.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "glob": "^11.0.1",
    "happy-dom": "^17.4.4",
    "sass-embedded": "^1.86.3",
    "tsx": "^4.19.3",
    "vite": "^6.3.0",
    "vitest": "^3.1.1",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8"
  }
}
