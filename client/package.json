{
  "name": "@geyser/client",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json",
    "typecheck:app": "vue-tsc --noEmit -p tsconfig.app.json",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:app",
    "codegen": "graphql-codegen",
    "codegen:schema": "graphql-codegen --config codegen.schema.ts"
  },
  "dependencies": {
    "@geyser/shared": "workspace:*",
    "@urql/core": "^6.0.1",
    "@urql/exchange-auth": "^3.0.0",
    "@urql/vue": "^2.0.0",
    "axios": "^1.12.2",
    "dompurify": "^3.2.7",
    "driver.js": "^1.3.6",
    "graphql": "^16.11.0",
    "marked": "^16.3.0",
    "papaparse": "^5.5.3",
    "quasar": "^2.18.5",
    "vue": "^3.5.21",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.50.1",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/client-preset": "^5.0.1",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@quasar/vite-plugin": "^1.10.0",
    "@types/papaparse": "^5.3.16",
    "@urql/devtools": "^2.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "glob": "^11.0.3",
    "happy-dom": "^18.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass-embedded": "^1.93.0",
    "vite": "^7.1.7",
    "vite-plugin-compression2": "^2.2.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  }
}
