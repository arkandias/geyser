{
  "name": "@geyser/client",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json",
    "typecheck:app": "vue-tsc --noEmit -p tsconfig.app.json",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:app",
    "codegen": "graphql-codegen",
    "codegen:schema": "graphql-codegen --config codegen.schema.ts"
  },
  "dependencies": {
    "@geyser/shared": "workspace:*",
    "@urql/core": "^6.0.1",
    "@urql/exchange-auth": "^3.0.0",
    "@urql/vue": "^2.0.0",
    "axios": "^1.13.3",
    "dompurify": "^3.3.1",
    "driver.js": "^1.4.0",
    "graphql": "^16.12.0",
    "marked": "^17.0.1",
    "papaparse": "^5.5.3",
    "quasar": "^2.18.6",
    "vue": "^3.5.27",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@quasar/vite-plugin": "^1.10.0",
    "@types/papaparse": "^5.5.2",
    "@urql/devtools": "^2.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "glob": "^13.0.0",
    "happy-dom": "^20.3.9",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-embedded": "^1.97.3",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.4.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
